<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.serviceModel>
		<bindings>
			<netTcpBinding>
				<binding name="NetTcpBinding_ISolarService"
						 transferMode="Streamed"
						 maxBufferSize="10485760"
						 maxReceivedMessageSize="10485760"
						 sendTimeout="00:00:30"
						 receiveTimeout="00:10:00">
					<security mode="None" />
					<reliableSession enabled="false"/>
				</binding>
			</netTcpBinding>
		</bindings>
		<client>
			<endpoint address="net.tcp://localhost:8088/SolarService"
					  binding="netTcpBinding"
					  bindingConfiguration="NetTcpBinding_ISolarService"
					  contract="Common.ISolarService"
					  name="NetTcpBinding_ISolarService" />
		</client>
	</system.serviceModel>
</configuration>